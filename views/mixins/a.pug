mixin link(href, name)
    a(href=href class="mx-2 text-white hover:underline hover:text-secondary transition-all duration-75")&attributes(attributes)= name
mixin a-button(href, name)
    a(href=href class="px-14 py-3 text-white rounded-full border-white border-2 hover:border-secondary transition-all duration-75")&attributes(attributes)= name